{
  "version": 2,
  "builds": [
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/web/index.html"
    }
  ],
  "buildCommand": "flutter build web --release --web-renderer html",
  "outputDirectory": "build/web",
  "installCommand": "flutter pub get"
}
